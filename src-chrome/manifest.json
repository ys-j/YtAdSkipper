{
	"manifest_version": 3,
	"name": "Yt Ad Skipper",
	"version": "0.1.0",
	"content_scripts": [
		{
			"js": ["content.js"],
			"matches": ["*://www.youtube.com/*"],
			"run_at": "document_end"
		}
	],
	"default_locale": "en",
	"description": "__MSG_manifestDescription__",
	"author":  "_y_s",
	"icons": {
		"16": "images/icon16.png",
		"32": "images/icon32.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	}
}